<div class="page-header">
  <%= link_to washers_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_washer_path(@washer), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show washer</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @washer.name %></dd>

  <dt>Position:</dt>
  <dd><%= @washer.position %></dd>

  <dt>Type:</dt>
  <dd><%= @washer.type %></dd>

  <dt>State:</dt>
  <dd><%= @washer.state %></dd>

  <dt>User:</dt>
  <dd><%= @washer.user %></dd>

</dl>
